<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/color_f2f3f2"
            >
            <RelativeLayout
                android:id="@+id/layout_money"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_200"
                android:background="@color/color_fe453a"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="提现金额"
                    android:gravity="center"
                    android:textSize="@dimen/sp_18"
                    android:textColor="@color/white"
                    android:layout_above="@+id/img_jt"
                    />

                <ImageView
                    android:id="@+id/img_jt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dp_30"
                    android:layout_marginTop="@dimen/dp_5"
                    android:layout_marginBottom="@dimen/dp_5"
                    />

                <TextView
                    android:id="@+id/tv_money"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="¥0.00元"
                    android:gravity="center"
                    android:textSize="@dimen/sp_35"
                    android:textColor="@color/white"
                    android:layout_below="@+id/img_jt"
                    />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_30"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="提现单号"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_odd_numbers"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="提现人编号"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_partner_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="提现日期"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_date"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_wall_hanging_install_amount"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="壁挂式净水机"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_wall_hanging_install_amount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0台"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_table_install_amount"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="台式净水机"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_table_install_amount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0台"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_vertical_install_amount"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="立式净水机"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_vertical_install_amount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0台"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="购买型合计（去押金）"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_purchase_total"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0元"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_wall_hanging_renew_amount"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_30"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="壁挂式净水机续费"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_wall_hanging_renew_amount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0单"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_table_renew_amount"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="台式净水机续费"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_table_renew_amount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0单"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_vertical_renew_amount"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="立式净水机续费"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_vertical_renew_amount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0单"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="续费型合计（去押金）"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_renew_total"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0元"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_30"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="是否按照合同"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_contract"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="否"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="返利比例"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_rebate_ratio"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="安装费比例"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_Installationfee_ratio"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="服务费返利"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_rebate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0元"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="服务费续费返利"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_renew_rebate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0元"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="安装费补贴"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_install_rebate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0元"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:paddingLeft="@dimen/dp_15"
                android:gravity="center_vertical"
                android:background="@color/white"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_1"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="下级返利"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    />

                <TextView
                    android:id="@+id/tv_subordinate_rebate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="0元"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:layout_marginRight="@dimen/dp_10"
                    android:gravity="end"
                    />
            </LinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/rcy"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                />
        </LinearLayout>
    </ScrollView>
</LinearLayout>
