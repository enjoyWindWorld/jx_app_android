<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/color_f2f3f2"
    >

    <LinearLayout
        android:id="@+id/layout_wall_hanging_install_amount"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="壁挂式净水机"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_wall_hanging_install_amount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0台"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_table_install_amount"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_1"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="台式净水机"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_table_install_amount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0台"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_vertical_install_amount"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_1"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="立式净水机"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_vertical_install_amount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0台"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_wall_hanging_renew_amount"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_30"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="壁挂式净水机续费"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_wall_hanging_renew_amount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0单"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_table_renew_amount"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_1"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="台式净水机续费"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_table_renew_amount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0单"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_vertical_renew_amount"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_1"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="立式净水机续费"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_vertical_renew_amount"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0单"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_30"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="去押金总金额"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_no_deposit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0元"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_1"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="返利比例"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_rebate_ratio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_1"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="服务费返利"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            />

        <TextView
            android:id="@+id/tv_sub_service"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0元"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:paddingLeft="@dimen/dp_15"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_1"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="服务费续费返利"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:id="@+id/textView2" />

        <TextView
            android:id="@+id/tv_sub_renew_service"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="0元"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:layout_marginRight="@dimen/dp_10"
            android:gravity="end"
            />
    </LinearLayout>

</LinearLayout>
