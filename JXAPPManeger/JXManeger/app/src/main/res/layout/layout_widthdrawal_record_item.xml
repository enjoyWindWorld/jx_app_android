<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical"
    android:layout_marginBottom="@dimen/dp_15"
    >

    <TextView
        android:id="@+id/tv_state"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="提现发起"
        android:gravity="center_vertical"
        android:textSize="@dimen/dp_16"
        android:textColor="@color/color_0f0f0f"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_20"
        />

    <TextView
        android:id="@+id/tv_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:textSize="@dimen/dp_12"
        android:textColor="@color/gray_deep"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_10"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="提现金额"
        android:textSize="@dimen/dp_18"
        android:textColor="@color/gray_deep"
        android:gravity="center"
        android:layout_marginTop="@dimen/dp_20"
        />


    <TextView
        android:id="@+id/tv_money"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="¥ 0.00元"
        android:padding="@dimen/dp_10"
        android:gravity="center"
        android:textSize="@dimen/sp_28"
        android:textColor="@color/color_333333"
        android:layout_toRightOf="@+id/img_state"
        android:layout_alignTop="@+id/img_state"
        android:layout_alignBottom="@+id/img_state"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/color_dddddd"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:paddingBottom="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_10"
        >
        <TextView
            android:layout_width="@dimen/dp_100"
            android:layout_height="wrap_content"
            android:text="提现方式"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />

        <TextView
            android:id="@+id/tv_mode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:paddingBottom="@dimen/dp_5"
        >
        <TextView
            android:layout_width="@dimen/dp_100"
            android:layout_height="wrap_content"
            android:text="提现时间"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />

        <TextView
            android:id="@+id/tv_start_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_audit_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:paddingBottom="@dimen/dp_5"
        >
        <TextView
            android:id="@+id/tv_audit_time_title"
            android:layout_width="@dimen/dp_100"
            android:layout_height="wrap_content"
            android:text="审核时间"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />

        <TextView
            android:id="@+id/tv_audit_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_arrive_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:paddingBottom="@dimen/dp_5"
        >
        <TextView
            android:id="@+id/tv_arrive_time_title"
            android:layout_width="@dimen/dp_100"
            android:layout_height="wrap_content"
            android:text="到账时间"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />

        <TextView
            android:id="@+id/tv_arrive_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_reason"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:paddingBottom="@dimen/dp_5"
        >
        <TextView
            android:layout_width="@dimen/dp_100"
            android:layout_height="wrap_content"
            android:text="失败原因"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />

        <TextView
            android:id="@+id/tv_reason"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:paddingBottom="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_10"
        >
        <TextView
            android:layout_width="@dimen/dp_100"
            android:layout_height="wrap_content"
            android:text="交易单号"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />

        <TextView
            android:id="@+id/tv_transaction_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_15"
            android:textColor="@color/color_0f0f0f"
            />
    </LinearLayout>

</LinearLayout>
